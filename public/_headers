/_astro/*
  Cache-Control: public, max-age=31536000, immutable

# 1. Security Headers for the whole game folder (Required for Godot 4)
/skillcheck/*
  Cross-Origin-Opener-Policy: same-origin
  Cross-Origin-Embedder-Policy: require-corp

# 2. Compression Rule (Required for the Gzip Trick)
# This allows you to upload the compressed ~6MB file but name it .wasm
/skillcheck/index.wasm
  Content-Encoding: gzip
  Content-Type: application/wasm